{
	"name": "fsg/oidc-azure",
	"type": "typo3-cms-extension",
	"description": "This extension replace the generic OAuth provider factory of causal/oidc by an Azure version.",
	"keywords": [
		"TYPO3",
		"OpenID",
		"OIDC",
		"Authentication",
		"Azure"
	],
	"homepage": "https://github.com/cjanody/t3ext-oidc-azure",
	"support": {
		"issues": "https://github.com/cjanody/t3ext-oidc-azure/issues"
	},
	"authors": [
		{
			"name": "Cyril Janody",
			"email": "cyril.janody@fsg.ulaval.ca",
			"role": "Developer"
		}
	],
	"license": "GPL-2.0-or-later",
	"require": {
		"php": "^8.3",
		"causal/oidc": "^4.0",
		"thenetworg/oauth2-azure": "^2.2"
	},
	"autoload": {
		"psr-4": {
			"FSG\\OidcAzure\\": "Classes/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"FSG\\OidcAzure\\Tests\\": "Tests/"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.x-dev"
		},
		"typo3/cms": {
			"extension-key": "oidc-azure"
		}
	}
}
